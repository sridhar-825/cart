<h1>Listing product_orders</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Order</th>
    <th>Product</th>
  </tr>

<% for product_order in @product_orders %>
  <tr>
    <td><%=h product_order.id %></td>
    <td><%=h product_order.order_id %></td>
    <td><%=h product_order.product_id %></td>
    <td><%= link_to 'Show', product_order %></td>
    <td><%= link_to 'Edit', edit_product_order_path(product_order) %></td>
    <td><%= link_to 'Destroy', product_order, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New product_order', new_product_order_path %>
